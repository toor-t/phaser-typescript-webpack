{
	"version": "2.0.0",
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": true,
		"outDir": "dist",
		"allowJs": true,
		// "strict": true,
		"strictNullChecks": true,
	},
	"exclude": [
		"node_modules"
	]
}